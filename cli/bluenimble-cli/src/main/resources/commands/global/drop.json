{

	"handlers": {
		"api": {
			"description": "uninstall an api from a bluenimble instance",
			
			"args": [{
				"name": "api", "desc": "api namespace to drop/uninstall"
			}],
		
			"request": {
				"method": "delete",
				"service": "<% keys.endpoints.management %>/apis/<% arg.0 %>"
			}
		}
	}

}