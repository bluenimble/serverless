{

	"handlers": {
		"space": {
			"description": "change a space runtime",
			
			"args": [{
				"name": "runtime", "desc": "a json variable containing the new runtime entries to add or update"
			}],
		
			"request": {
				"method": "put",
				"service": "[ keys.endpoints.management ]/spaces/NONE",
				"contentType": "application/json",
				"body": {
					"payload": "[arg.0]"
				}
			}
		}
	}

}