{
	"handlers": {
		"entity": {
			"description": "create an entity",
			
			"args": [{
				"name": "entity", "desc": "database entity name"
			}],
		
			"request": {
				"method": "post",
				"service": "<% keys.endpoints.management %>/db/<% vars.database.provider %>/entities/<% arg.0 %>"
			}
		}
	}
}