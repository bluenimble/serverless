{

	"statusManager": {
		"class": "com.bluenimble.platform.api.impls.DefaultStatusManager",
		"properties": {
			"readOnly": "[ this.readOnly | 'false' ]",
			"delay": "[ this.delay | '0' ]",
			"period": "[ this.period | '20' ]"
		}
	},
	
	"secrets": {
		"default": {
			"key": "", "algorithm": "AES", "age": "60"
		}
	},
	
	"features": {
		"scripting": {
			"default": {
				"provider": "scripting.javascript"
			}
		}
	},
	
	"tracer": {
		"class": "!tracing.log4j:default"
	},
	
	"runtime": {
		"timeout": "10000"
	}
  
}