{
	"namespace": "sys",
	"name": "BlueNimble SysAdmin Space",
	
	"statusManager": {
		"class": "com.bluenimble.platform.api.impls.DefaultStatusManager",
		"properties": {
			"readOnly": "[ this.readOnly | 'false' ]",
			"delay": "[ this.delay | '0' ]",
			"period": "[ this.period | '20' ]"
		}
	},
	
	"features": {
		"scripting": {
			"default": {
				"provider": "bnb-javascript"
			}
		}
	},
	
	"tracer": {
		"class": "!tracing.log4j:default"
	},
	
	"runtime": {
		"timeout": "10000"
	}
	
}